QFrame#DisplayModeFrame{
    background-color: transparent;
}

QFrame#DisplayModeFrame > BaseCheckedButton#ViewModeButton{
    border-image: url(:/skin/images/category_50.svg);
}

QFrame#DisplayModeFrame > BaseCheckedButton#ViewModeButton:hover{
    border-image: url(:/skin/images/category_100.svg);
}


BaseCheckedButton{
    background-color: transparent;
    color: rgba(255, 255, 255, 100);
    border:none;
    text-align: left, bottom;
}

BaseCheckedButton:hover{
    background-color: transparent;
    border:none;
    color: white;
    text-align: left, bottom;
}


BaseCheckedButton:checked{
    background-color: transparent;
    border:none;
    color: rgba(67, 255, 255, 255);
    text-align: left bottom;
}


BaseCheckedButton#CategoryTextButton{
    /*background-color: transparent;*/
    color: rgba(255, 255, 255, 100);
    padding-left: 0px;
    border:2px red solid;
    font-size: 16px;
    text-align: left center;
}

BaseCheckedButton#CategoryTextButton:hover{
    background-color: transparent;
    border:none;
    padding-left: 0px;
    text-align: left center;
}


BaseCheckedButton#CategoryTextButton:checked{
    background-color: transparent;
    border:none;
    color: white;
    padding-left: 0px;
    text-align: left center;
}



